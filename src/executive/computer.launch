<launch>		
	<node name="perception_master"  pkg="perception"  type="perception_node_simple.py"/>
	<node name="perception_webcam" pkg="perception" type="webcam3.py" output="screen" />
	<node name="perception_lidar" pkg="perception" type="perception_lidar.py" output="screen" />
	<!-- need to add ultrasonic -->
	<node name="delta_lidar"    pkg="delta_lidar"  type="delta_lidar_node" output = "screen">
    		<param name="serial_port"         type="string" value="/dev/ttyUSB0"/>
    		<param name="frame_id"            type="string" value="laser"/>
  	</node>
	<!--node name="rviz" pkg="rviz" type="rviz" args="-d $(find delta_lidar)/rviz/delta_lidar.rviz" /-->
	<node type="robot_pose.py" name="robot_pose_node" pkg="localization"/>
	<node type="gps_convertion_server.py" name="gps_convertion_server" pkg="localization"/>
	<node type="navigation_node.py" name="navigation_node" pkg="navigation"/>
	<!--node type="path_planning_node.py" name="path_planning_node" pkg="path_planning" output="screen"/-->
</launch>

